overwrite: true
schema: '<%= schemaLocation %>'
<%_ if (typeDefinition === 'GraphQL') {  _%>
documents: 'src/**/*.graphql'
<%_ } _%>
generates:
  src/main/webapp/app/graphql.ts:
    plugins:
      - 'typescript'
<%_ if (typeDefinition === 'GraphQL') {  _%>
      - 'typescript-operations'
<%_ } _%>
<%_ if (typeDefinition === 'GraphQL' && clientFramework === 'angularX') {  _%>
      - 'typescript-apollo-angular'
<%_ } _%>
<%_ if (typeDefinition === 'GraphQL' && (clientFramework === 'react' || clientFramework === 'vue')) {  _%>
      - 'typescript-react-apollo'
<%_ } _%>
<%_ if (typeDefinition === 'TypeScript') {  _%>
      - 'graphql-typeop/plugins'
<%_ } _%>
    config:
      skipTypename: true
      maybeValue: T | undefined
      withHooks: false
